{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "SLEEP_BETWEEN_DOWNLOAD_TRIES_SECONDS": "300",
                "SRC_FILE_TO_DOWNLOAD_URL": "https://example.com/file.txt",
                "DEST_DOWNLOAD_DIR": "/data",
                "LOG_LEVEL": "DEBUG"
            }
        }
    ]
}
